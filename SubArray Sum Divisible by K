def subarray_sum(nums, k):
    count = 0
    n = len(nums)

    for i in range(n):
        curr_sum = 0
        for j in range(i, n):
            curr_sum += nums[j]
            if curr_sum % k == 0:
                count += 1

    return count
    

nums = [4, 5, 0, -2, -3, 1]
k = 5
print(subarray_sum(nums, k))
